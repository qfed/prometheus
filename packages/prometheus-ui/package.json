  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "doc:watch": "node bin/doc-watch.js & vuepress dev docs",
    "doc:dev": "node bin/doc-pre.js && npm run doc:watch",
    "doc:build": "node bin/doc-pre.js && vuepress build docs",
    "deploy": "npm run doc:build",
    "doc:start": "npm run deploy && node ./bin/doc-start.js",
    "build": "cross-env NODE_ENV=development node bin/build.js && cross-env NODE_ENV=development gulp",
    "build:prod": "cross-env NODE_ENV=development node bin/build.js && cross-env NODE_ENV=production gulp",
    "dev": "nodemon ./node_modules/.bin/gulp start",
    "prepublish": "npm run build:prod",
    "start": "gulp start --silent",
    "theme": "npm run build:prod   && git add .&& git commit -m \"update dist\" &&  git push && cd ../q-react &&yarn add git+ssh://git@git.qufenqi.com:fe/q-ui.git#fix_input-style -D && cd ../q-ui"
  },
